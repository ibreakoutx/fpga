#!/bin/sh -e
# This file is automatically generated by VCS.  Any changes you make
# to it will be overwritten the next time VCS is run.
/tools/vcs/linux/bin/vcs1 -Xrebuild '-Mcfl= -pipe -O -I/tools/vcs/include ' -Mout=simv -Mrun=/tools/vcs/linux/lib/libvcs.a -Mamsrun= -Mvcsaceobjs= '-Mscsrun= ' -Mcmodrun= '-Mobjects= /tools/vcs/linux/lib/libvirsim.a ' -Mcrtn= -Mcsrc= '-Msyslibs=/tools/vcs/linux/lib/ctype-stubs_32.a -ldl ' gdp.v +v2k +define+TEST -sverilog -gen_obj 2>&1
cd csrc 2>&1
make  -f Makefile  CFLAGS=' -pipe  -O -I/tools/vcs/include'      DEFAULT_RUNTIME=TRUE product 2>&1
exit $?
